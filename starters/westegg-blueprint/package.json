{
  "name": "westegg-blueprint",
  "description": "Westegg City Limits",
  "version": "1.1.3",
  "author": "Andrew Alexander (adrw)",
  "license": "MIT",
  "engines": {
    "node": ">=7.8.0",
    "yarn": "YARN NO LONGER USED - use npm instead."
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:adrw/westegg.git"
  },
  "scripts": {
    "clean": "rimraf .cache public",
    "start:app": "npm run develop",
    "start:lambda": "netlify-lambda serve src/lambda",
    "start": "run-p start:**",
    "build:app": "npm run clean && gatsby build",
    "build:lambda": "netlify-lambda build src/lambda",
    "build": "run-p build:**",
    "develop": "npm run clean && gatsby develop",
    "lint": "prettier --write --config package.json \".{/src/**/,/}*.{md,css,less,scss,json,js,jsx,ts,tsx}\"",
    "serve": "gatsby serve",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.15.1",
    "bulma": "^0.7.4",
    "gatsby": "^2.3.4",
    "gatsby-image": "^2.0.35",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-netlify-cms": "^3.0.17",
    "gatsby-plugin-purgecss": "^3.1.1",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "lodash": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.5",
    "netlify-cms": "^2.6.1",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.12.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "uuid": "^3.2.2",
    "webpack-sources": "^1.3.0"
  },
  "devDependencies": {
    "@westegg/dev": "0.0.2-3",
    "core-js": "^3.0.1",
    "http-proxy-middleware": "^0.19.1",
    "netlify-lambda": "^1.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.2"
  },
  "greenkeeper": {
    "ignore": [
      "core-js"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
}
